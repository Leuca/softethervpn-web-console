{
  "name": "softethervpn-web-console",
  "version": "0.0.1",
  "description": "A SoftEther VPN Server web management application.",
  "repository": "https://github.com/Leuca/softethervpn-web-console.git",
  "homepage": "https://github.com/Leuca/softethervpn-web-console",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --color --progress --config webpack.dev.js",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "devDependencies": {
    "@axe-core/react": "^4.4.2",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/addons": "^6.5.8",
    "@storybook/react": "^7.5.1",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^26.0.19",
    "@types/react-router-dom": "^5.1.7",
    "@types/victory": "^33.1.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^5.2.4",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "dotenv-webpack": "^7.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^8.12.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "imagemin": "^7.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.6.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.36.2",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@patternfly/react-core": "^4.250.1",
    "@patternfly/react-icons": "^4.92.6",
    "@patternfly/react-styles": "^4.91.6",
    "@patternfly/react-table": "^4.111.4",
    "@peculiar/x509": "^1.3.0",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "path-browserify": "^0.0.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-last-location": "^2.0.1",
    "renderkid": "^2.0.7",
    "sirv-cli": "^1.0.10",
    "stream-browserify": "^2.0.2",
    "stream-http": "^2.8.3",
    "vpnrpc": "^1.0.1"
  }
}
